{
    "name": "thiiagoms/isp-php",
    "description": "Interface Segregation Principle (ISP) in PHP for Composer Packages:",
    "autoload": {
        "psr-4": {
            "Thiiagoms\\ISP\\": "src/"
        }
    },
    "authors": [
        {
            "name": "thiiagoms",
            "email": "thiagom.devsec@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^11",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "phpcs": "phpcs --standard=PSR12",
        "phpcbf": "phpcbf --standard=PSR12",
        "tests": "phpunit"
    },
    "require": {
        "illuminate/support": "^11.21"
    }
}
